{
	"name": "antiage-ui-kit",
	"version": "0.0.0-development",
	"description": "A react component library Anti-Age",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "husky install",
		"build": "rollup -c",
		"test": "jest --coverage",
		"size": "size-limit",
		"lint": "eslint . --fix --ext .ts,.tsx",
		"stylelint": "stylelint \"src/**/*.css\" --fix",
		"semantic-release": "semantic-release",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"generate": "node ./utils/create-component",
		"svgr": "npx @svgr/cli --template \"utils/templates/svg.js\" --index-template \"utils/templates/index-svg.js\" --svgo-config svgo.config.js --out-dir \"src/assets\" --ignore-existing --typescript -- \"src/assets\""
	},
	"lint-staged": {
		"*.css": "stylelint --fix",
		"*.{js,jsx,ts,tsx}": "eslint --fix"
	},
	"author": "IT_DOGTOR",
	"license": "MIT",
