{
	"name": "antiage-ui-kit",
	"version": "0.0.0-development",
	"description": "A react component library Anti-Age",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "husky install",
		"build": "rollup -c",
		"commitlint": "commitlint --edit",
		"test": "jest --coverage",
		"size": "size-limit",
		"eslint": "eslint src/ --fix",
		"stylelint": "stylelint \"src/**/*.css\" --fix",
		"semantic-release": "semantic-release",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"generate": "node ./utils/create-component",
		"svgr": "npx @svgr/cli --template \"utils/templates/svg.js\" --index-template \"utils/templates/index-svg.js\" --svgo-config svgo.config.js --out-dir \"src/assets\" --ignore-existing --typescript -- \"src/assets\" && node utils/remove-svgs.js"
	},
	"lint-staged": {
		"*.css": "stylelint --fix",
		"*.{js,jsx,ts,tsx}": "eslint --fix"
	},
	"author": "IT_DOGTOR",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.23.3",
		"@babel/preset-env": "^7.23.3",
		"@babel/preset-react": "^7.23.3",
		"@babel/preset-typescript": "^7.23.3",
		"@commitlint/cli": "^18.4.1",
		"@commitlint/config-conventional": "^18.4.0",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@size-limit/preset-small-lib": "^8.1.0",
		"@storybook/addon-actions": "^6.5.10",
		"@storybook/addon-essentials": "^6.5.10",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/addon-viewport": "^6.5.10",
		"@storybook/addons": "^6.5.10",
		"@storybook/builder-webpack5": "^6.5.10",
		"@storybook/manager-webpack5": "^6.5.10",
		"@storybook/react": "^6.5.10",
		"@storybook/theming": "^6.5.10",
		"@svgr/cli": "^6.5.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.1.1",
		"@types/lodash.debounce": "^4.0.7",
		"@types/lodash.range": "^3.2.7",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"antd": "^4.23.4",
		"babel-jest": "^29.7.0",
		"babel-loader": "^9.1.3",
		"classnames": "^2.3.2",
		"colors": "^1.4.0",
		"commitizen": "^4.3.0",
		"core-js": "^3.33.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.53.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jest": "^27.6.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-storybook": "^0.6.15",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.1.2",
		"jest-environment-jsdom": "^29.1.2",
		"jest-watch-typeahead": "^2.2.0",
		"lint-staged": "^15.1.0",
		"lodash.debounce": "^4.0.8",
		"lodash.range": "^3.2.0",
		"prettier": "^3.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-phone-input-2": "^2.15.1",
		"react-router-dom": "^6.4.1",
		"react-select": "^5.7.2",
		"rollup": "^4.4.1",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-import-css": "^3.3.5",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-typescript2": "^0.36.0",
		"semantic-release": "^22.0.7",
		"size-limit": "^11.0.0",
		"storybook-addon-react-router-v6": "^0.2.1",
		"stylelint": "^15.11.0",
		"stylelint-config-clean-order": "^5.2.0",
		"stylelint-config-css-modules": "^4.3.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-prettier": "^4.0.2",
		"typescript": "^5.2.2",
		"webpack": "^5.74.0"
	},
	"peerDependencies": {
		"antd": ">=4",
		"classnames": ">=2",
		"react": ">=16",
		"react-phone-input-2": ">=2",
		"react-router-dom": ">=5",
		"react-select": ">=4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ITDOGTOR/antiage-ui-kit.git"
	},
	"size-limit": [
		{
			"path": "dist/index.js",
			"limit": "1024 KB"
		},
		{
			"path": "dist/index.esm.js",
			"limit": "1024 KB"
		}
	],
	"engines": {
		"node": ">=16"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog",
			"disableScopeLowerCase": true,
			"types": {
				"feat": {
					"description": "Adding new feature"
				},
				"fix": {
					"description": "Bug fix or defect"
				},
				"docs": {
					"description": "Documentation Update"
				},
				"refactor": {
					"description": "Editing code without fixing bugs or adding new features"
				},
				"chore": {
					"description": "Add / update / customize tools and libraries"
				},
				"perf": {
					"description": "Editing code to improve performance"
				},
				"test": {
					"description": "Add / update / test"
				}
			}
		}
	}
}
